{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "browser_action": {
        "default_popup": "view.html"
    },
    "content_scripts": [
        {
            "matches": [
              "https://*.atlassian.net/*"
            ],
            "js": [
                "Content.js"
            ],
            "all_frames": true
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}